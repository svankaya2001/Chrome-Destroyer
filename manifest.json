{
    "manifest_version": 2,

    "name": "Google to Bing",
    "version": "0.1",

    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*.google.com/*",
        "*://*.duckduckgo.com/*", 
        "*://*.yahoo.com/*",
        "*://*.baidu.com/*",
        "*://*.youtube.com/*",
        "storage"
    ],
    "background": {"scripts":["redirect.js"]},
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "run_at": "document_end",
            "js": ["alterLinks.js", "alterImages.js"],
            "all_frames": true
         }
    ]

}