{
    "manifest_version": 2,

    "name": "Google to Bing",
    "version": "0.1",

    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*.google.com/*",
        "*://*.duckduckgo.com/*", 
        "*://*.yahoo.com/*",
        "*://*.baidu.com/*"
    ],
    "background": {"scripts":["redirect.js"]},
    "content_scripts": [
        {
            "matches": ["https://www.bing.com/*"],
            "js": ["alterLinks.js"] 
         }
    ]

}